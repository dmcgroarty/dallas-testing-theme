<h3>{{ section.settings.heading }}</h3>
<p>{{ section.settings.description }}</p>

{% schema %}
{
  "name": "Testing",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading for section"
    },
    {
      "type": "rich-text",
      "label": "description",
      "id": "description"
    }
  ]
}
{% endschema %}