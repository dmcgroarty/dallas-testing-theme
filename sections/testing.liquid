

{% for block in section.blocks %}
  <img src="{{ block.settings.image_example | image_url: 'master' }}" />
{% endfor %}

{% schema %}
{
  "name": "testing",
  "blocks": [
    {
      "type": "image",
      "name": "image block",
      "settings": [
        {
          "type": "image_picker",
          "label": "Your image",
          "id": "image_example"
        }
      ]
    }
  ]
}
{% endschema %}